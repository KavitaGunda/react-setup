{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      // Lets you use async/await feature in your react app
      "@babel/plugin-transform-runtime", 
      {
        "regenerator": true
      }
    ]
  ]
}